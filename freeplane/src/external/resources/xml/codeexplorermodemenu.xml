<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<FreeplaneUIEntries>
	<Entry name="main_toolbar" builder="toolbar,ignore">
		<Entry builder="separator" />
		<Entry builder="main_toolbar_zoom" name="zoom" row="1" width = "2"/>
		<Entry name="PrintAction" row="2"/>
		<Entry name="CloseAction" row="2"/>
		<Entry name="code.CopyQualifiedName" row="1"/>
		<Entry name="code.RunAnalysisAction" row="2"/>
		<Entry builder="separator" />
		<Entry row="1">
			<Entry name="JumpInAction"/>
			<Entry name="UnfoldOneLevelAction" />
			<Entry name="UnfoldAllAction" />
		</Entry>
		<Entry row="2">
			<Entry name="JumpOutAction" />
			<Entry name="FoldOneLevelAction" />
			<Entry name="FoldAllAction" />
		</Entry>
		<Entry builder="separator" />
		<Entry name="SetBooleanPropertyAction.code_showOutsideDependencies" />
		<Entry row="1">
			<Entry name="code.ShowSelectedClassesWithExternalDependenciesAction" />
		</Entry>
		<Entry row="2">
			<Entry name="code.ShowAllClassesAction" />
		</Entry>
		<Entry builder="separator" />
		<Entry row="1">
			<Entry name="code.FilterCyclesAction" />
		</Entry>
		<Entry row="2">
			<Entry name="code.SelectCyclesAction" />
		</Entry>
		<Entry builder="separator" />
		<Entry row="1">
			<Entry name="code.ShowDependingNodesAction.INCOMING.SELECTED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.SELECTED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.SELECTED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.SELECTED.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.SELECTED.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.SELECTED.MAXIMUM_RECURSION" />
		</Entry>
		<Entry row="2">
			<Entry name="code.ShowDependingNodesAction.INCOMING.VISIBLE.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.VISIBLE.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.VISIBLE.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.VISIBLE.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.VISIBLE.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.VISIBLE.MAXIMUM_RECURSION" />
		</Entry>
		</Entry>
		<Entry name="main_menu" builder="main_menu">
		<Entry name="file">
			<Entry name="lastOpenedMaps">
				<Entry builder="lastOpenedMaps" />
			</Entry>
			<Entry name="PageAction" />
			<Entry name="PrintPreviewAction" />
			<Entry accelerator="control P" name="PrintAction" />
			<Entry accelerator="control W" name="CloseAction" />
			<Entry accelerator="control Q" name="QuitAction" builder="MB_QuitAction" />
		</Entry>
		<Entry name="edit">
			<Entry name="code.CopyQualifiedName" accelerator="control C"/>
			<Entry name="find">
				<Entry accelerator="control G" name="FindAction" />
				<Entry name="FindNextAction"  accelerator="control shift G" />
				<Entry name="FindPreviousAction" />
				<Entry name="OpenPathAction" />
			</Entry>
		</Entry>
		<Entry name="view">
			<Entry name="zoom">
				<Entry accelerator="alt UP" name="ZoomInAction" />
				<Entry accelerator="alt DOWN" name="ZoomOutAction" />
			</Entry>
			<Entry name="menu_scroll_map">
				<Entry name="FreeScrollAction.LEFT" />
				<Entry name="FreeScrollAction.UP" />
				<Entry name="FreeScrollAction.RIGHT" />
				<Entry name="FreeScrollAction.DOWN" />
				<Entry accelerator="alt W" name="MoveSelectedNodeAction.LEFT" />
				<Entry accelerator="alt C" name="MoveSelectedNodeAction.CENTER" />
				<Entry accelerator="alt E" name="MoveSelectedNodeAction.RIGHT" />
				<Entry name="MoveSelectedNodeAction.TOP" />
				<Entry name="MoveSelectedNodeAction.BOTTOM" />
				<Entry name="MoveSelectedNodeAction.TOP_LEFT" />
				<Entry name="MoveSelectedNodeAction.TOP_RIGHT" />
				<Entry name="MoveSelectedNodeAction.BOTTOM_LEFT" />
				<Entry name="MoveSelectedNodeAction.BOTTOM_RIGHT" />
			</Entry>
			<Entry name="controls">
				<Entry name="ToggleMenubarAction" builder="MB_ToggleMenubarAction" />
				<Entry name="ToggleToolbarAction" />
				<Entry name="ShowFilterToolbarAction" accelerator="control F" />
				<Entry name="ToggleScrollbarsAction" />
				<Entry name="ToggleMapOverviewAction" />
				<Entry name="SetBooleanPropertyAction.spotlight" />
			</Entry>
			<Entry name="menu_viewmode">
				<Entry name="ShowSelectionAsRectangleAction" />
				<Entry name="SetBooleanPropertyAction.show_icons" />
				<Entry name="SetBooleanPropertyAction.show_connectors" />
				<Entry name="SetBooleanPropertyAction.hide_single_end_connectors" />
				<Entry name="SetBooleanPropertyAction.show_note_icon_in_tooltip" />
				<Entry name="SetBooleanPropertyAction.slow_scroll_selected_node" />
				<Entry name="map_overview" >
					<Entry name="SetStringPropertyAction.map_overview_bounds." />
					<Entry builder="radio_button_group" name="map_overview_attach_point" textKey="map_overview_attach_point">
						<Entry name="SetStringPropertyAction.map_overview_attach_point.SOUTH_EAST" />
						<Entry name="SetStringPropertyAction.map_overview_attach_point.SOUTH_WEST" />
						<Entry name="SetStringPropertyAction.map_overview_attach_point.NORTH_EAST" />
						<Entry name="SetStringPropertyAction.map_overview_attach_point.NORTH_WEST" />
					</Entry>
				</Entry>
			</Entry>
			<Entry name="SetBooleanPropertyAction.code_showOutsideDependencies" />
		</Entry>
		<Entry name="navigate" builder="menu_navigate">
			<Entry name="NavigationPreviousMapAction" />
			<Entry name="NavigationNextMapAction" accelerator="control TAB" />
			<Entry name="fold">
				<Entry name="ToggleFoldedAction" accelerator="SPACE" />
				<Entry name="ShowNextChildAction" accelerator="shift SPACE" />
				<Entry name="ToggleChildrenFoldedAction" accelerator="control SPACE" />
				<Entry name="UnfoldOneLevelAction" accelerator="alt PAGE_DOWN" />
				<Entry name="FoldOneLevelAction" accelerator="alt PAGE_UP" />
				<Entry name="UnfoldAllAction" accelerator="alt END" />
				<Entry name="FoldAllAction" accelerator="alt HOME" />
				<Entry builder="separator" />
				<Entry name="SetAlwaysUnfoldedNodeFlagsAction" />
				<Entry name="RemoveAllAlwaysUnfoldedNodeFlagsAction" />
				<Entry name="AlwaysUnfoldedNodeAction" />
			</Entry>
			<Entry builder="separator" />
			<Entry name="SelectAllAction" accelerator="control A" />
			<Entry name="SelectBranchAction" accelerator="control shift A" />
			<Entry builder="separator" />
			<Entry name="MoveToRootAction" accelerator="ESCAPE" />
			<Entry name="GotoNodeAction" />
			<Entry name="JumpInAction"  accelerator="shift ESCAPE"/>
			<Entry name="JumpOutAction"  accelerator="control ESCAPE"/>
			<Entry name="FreeSelectNodeAction.LEFT" />
			<Entry name="FreeSelectNodeAction.UP" />
			<Entry name="FreeSelectNodeAction.RIGHT" />
			<Entry name="FreeSelectNodeAction.DOWN" />
			<Entry name="NextNodeAction.BACK" accelerator="control alt LEFT" />
			<Entry name="NextNodeAction.FORWARD" accelerator="control alt RIGHT" />
			<Entry name="NextNodeAction.BACK_N_FOLD" accelerator="control shift LEFT" />
			<Entry name="NextNodeAction.FORWARD_N_FOLD" accelerator="control shift RIGHT" />
			<Entry name="NotFoldingNextPresentationItemAction" accelerator="control alt SPACE" />
			<Entry name="NextPresentationItemAction" accelerator="control shift SPACE" />
			<Entry name="BackAction" accelerator="alt LEFT" />
			<Entry name="ForwardAction" accelerator="alt RIGHT" />
			<Entry name="links">
				<Entry name="FollowLinkAction" accelerator="control ENTER" />
				<Entry builder="menu_goto_links" />
			</Entry>
		</Entry>
		<Entry name="code.explore">
			<Entry name="code.RunAnalysisAction" accelerator="control R"/>
			<Entry builder="separator" />
			<Entry name="code.ShowSelectedClassesWithExternalDependenciesAction" />
			<Entry name="code.ShowAllClassesAction" />
			<Entry name="code.FilterCyclesAction" />
			<Entry name="code.SelectCyclesAction" />
			<Entry builder="separator" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.SELECTED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.SELECTED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.SELECTED.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.SELECTED.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.SELECTED.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.SELECTED.MAXIMUM_RECURSION" />
			<Entry builder="separator" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.VISIBLE.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.VISIBLE.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.VISIBLE.NO_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING.VISIBLE.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.OUTGOING.VISIBLE.MAXIMUM_RECURSION" />
			<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.VISIBLE.MAXIMUM_RECURSION" />
		</Entry>
		<Entry name="filter">
			<Entry name="UndoFilterAction" />
			<Entry name="RedoFilterAction" />
			<Entry name="ReapplyFilterAction" />
			<Entry name="QuickFilterAction" />
            <Entry name="QuickAndFilterAction" />
            <Entry name="QuickOrFilterAction" />
			<Entry name="ApplySelectedViewConditionAction" />
			<Entry name="QuickFindAllAction" />
			<Entry name="ApplyNoFilteringAction" />
			<Entry name="EditFilterAction" />
			<Entry builder="separator" />
            <Entry name="HideMatchingNodesAction" />
			<Entry name="ShowAncestorsAction" />
			<Entry name="ShowDescendantsAction" />
			<Entry name="ApplyToVisibleAction" />
			<Entry builder="separator" />
			<Entry name="QuickHighlightAction" />
			<Entry name="QuickFindAction.BACK" />
			<Entry name="QuickFindAction.FORWARD" />
		</Entry>
		<Entry name="mindmaps">
			<Entry name="modes" builder="navigate_modes,radio_button_group" />
			<Entry builder="separator" />
			<Entry name="maps" processOnPopup="true">
				<Entry builder="navigate_maps,radio_button_group" />
			</Entry>
		</Entry>
		<Entry name="help" textKey="menu_help">
            <Entry name="CommandSearchAction" accelerator="control F1" />
            <Entry builder="separator" />
			<Entry name="web_resources">
				<Entry name="OpenFreeplaneSiteAction" />
                <Entry name="AskForHelp" />
				<Entry name="ReportBugAction" />
				<Entry name="RequestFeatureAction" />
			</Entry>
			<Entry name="AboutAction" />
			<Entry accelerator="F1" name="DocumentationAction" />
            <Entry name="FreeplaneHandbook" />
            <Entry name="HotKeyInfoAction" />
		</Entry>
	</Entry>
	<Entry name="map_popup" builder="map_popup" processOnPopup="true">
		<Entry builder="navigate_maps,radio_button_group" />
		<Entry builder="separator" />
		<Entry name="SetBooleanPropertyAction.code_showOutsideDependencies" />
		<Entry builder="separator" />
		<Entry name="ToggleMenubarAction" />
		<Entry name="ToggleToolbarAction" />
		<Entry name="ShowFilterToolbarAction" />
		<Entry name="ToggleScrollbarsAction" />
		<Entry name="ToggleMapOverviewAction" />
		<Entry accelerator="ESCAPE" name="MoveToRootAction" />
	</Entry>
	<Entry name="node_popup" builder="node_popup">
		<Entry name="JumpInAction"/>
		<Entry name="JumpOutAction" />
		<Entry name="code.CopyQualifiedName" accelerator="control C"/>
		<Entry builder="separator" />
		<Entry name="code.ShowSelectedClassesWithExternalDependenciesAction" />
		<Entry name="code.FilterCyclesAction" />
		<Entry name="code.ShowAllClassesAction" />
		<Entry name="code.SelectCyclesAction" />
		<Entry builder="separator" />
		<Entry name="code.ShowDependingNodesAction.INCOMING.SELECTED.NO_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.OUTGOING.SELECTED.NO_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.SELECTED.NO_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.INCOMING.SELECTED.MAXIMUM_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.OUTGOING.SELECTED.MAXIMUM_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.SELECTED.MAXIMUM_RECURSION" />
		<Entry builder="separator" />
		<Entry name="code.ShowDependingNodesAction.INCOMING.VISIBLE.NO_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.OUTGOING.VISIBLE.NO_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.VISIBLE.NO_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.INCOMING.VISIBLE.MAXIMUM_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.OUTGOING.VISIBLE.MAXIMUM_RECURSION" />
		<Entry name="code.ShowDependingNodesAction.INCOMING_AND_OUTGOING.VISIBLE.MAXIMUM_RECURSION" />
		<Entry builder="separator" />
		<Entry name="goto" processOnPopup="true">
			<Entry builder="link_actions" />
		</Entry>
	</Entry>
</FreeplaneUIEntries>
